{
    "sphinx": {
        "enabled": true,
        "cq_default_path": "doc/source/conf.py",
        "min": "$MIN_SPHINX_WARNINGS",
        "max": "${MAX_SPHINX_WARNINGS}"
    },
    "doxygen": {
        "enabled": true,
        "cq_default_path": "doc/doxygen/Doxyfile",
        "min": "0",
        "max": 0
    },
    "junit": {
        "enabled": false,
        "min": 0,
        "max": "$NONEXISTENT_ENVVAR"
    },
    "xmlrunner": {
        "enabled": true,
        "min": 0,
        "max": 0
    },
    "coverity": {
        "enabled": true,
        "cq_default_path": "dummy/path",
        "intentional": {
            "min": 0,
            "max": "-1"
        },
        "bug": {
            "min": 0,
            "max": 0
        },
        "pending": {
            "min": 0,
            "max": 0
        },
        "false_positive": {
            "min": 0,
            "max": -1
        }
    },
    "robot": {
        "enabled": false,
        "suites": [
            {
                "name": "Suite name 1",
                "min": 10,
                "max": 10
            },
            {
                "name": "Suite name 2",
                "min": 0,
                "max": 0
            }
        ]
    },
    "polyspace": {
        "enabled": false
    }
}
